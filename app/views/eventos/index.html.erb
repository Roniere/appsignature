<h1 class="eventos">Eventos Signature</h1>
<h2><%= link_to 'Novo Evento', new_evento_path %></h2>
<table border="0" width="100%" class="eventos">
<% @eventos.each do |evento| %>
	<tr>
		<td class="eventos" rowspan="3">
			<%= link_to image_tag(evento.fotoevento1.url(:medium)), evento%>
		
		</td>
		
	</tr>
	<tr>
		<td class="eventos">
				<h2 class="eventos">
						<%= link_to evento.title, evento%>
				</h2>
		</td>
	</tr>
	<tr>
		<td class="eventos"><%= evento.descricaoevento %></td>
	</tr>
	<tr>
		<td colspan="3">
		<%= link_to 'Editar', edit_evento_path(evento) %>
		<%= link_to 'Excluir', evento, :confirm => 'Deseja Realmente Excluir?', :method => :delete %></td>
	</tr>
<% end %>
</table>

<br />


